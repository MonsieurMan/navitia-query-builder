{
  "name": "navitia-query-builder",
  "version": "1.0.0",
  "description": "Fluently build Navitia query URLs",
  "main": "dist/index.js",
  "repository": "https://github.com/MonsieurMan/navitia-query-builder.git",
  "author": "MonsieurMan",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx,.spec.ts}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.spec.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "testRegex": "(/src/.*|(\\.|/)(spec))\\.spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}